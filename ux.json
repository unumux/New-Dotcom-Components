{
    "scss": {
        "src": "components/**/*.scss",
        "dest": "style"
    },
    "js": {
        "src": "scripts/**/*.js",
        "dest": "scripts",
        "main": "scripts/site.js"
    },
    "watch": [
        "components/**/*.html"
    ],
    "server": true,
    "compileJs": true,
    "proxy": false
}